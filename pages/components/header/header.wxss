/* 顶部导航 */

.status-bar {
  width: 100%;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  background: #fff;
}

.goBack {
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 10px 15px;
  z-index: 11
}

.goBack image {
  width: 40rpx;
  height: 40rpx;
  /* padding-left: 25rpx; */
}

.tabar {
  display: flex;
  justify-content: center;
  /* background-color: #fff; */
}

.tabar2 {
  background: transparent !important;
}

.tabar2 text {
  color: #fff !important;
  font-weight: lighter !important;
}

.tabar text {
  width: 100%;
  height: 46px;
  line-height: 46px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family:PingFang-SC-Medium;
  font-size:28rpx;
}

.tabar .active {
  color: #fff;
}
